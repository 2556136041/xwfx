
<form bindsubmit='formsubmit' report-submit='true'>
<input name="id" type='text' placeholder='交易单号'></input>
<input name="name" type='text' placeholder='物品名称'></input>
<picker name="date" mode="date" value="{{date}}" start="2017-1-1" end="2020-12-31" bindchange='bindDateChange'>
<view class="pick">购买时间：{{date}}</view>
</picker>
<input name="place" type="text" placeholder='购买地点'></input>
<button form-type='submit'>submit</button>

</form>